    <string name="timesync_sync_now_progress_dialog_details">Verbinden met server…</string>
    <string name="timesync_sync_now_time_corrected_dialog_title">Tijd gecorrigeerd</string>
    <string name="timesync_sync_now_time_corrected_dialog_details">"Interne klok Authenticator's is aangepast aan de tijd van de servers. De datum &amp; Tijd instellingen op dit apparaat zijn niet gewijzigd."</string>
    <string name="timesync_sync_now_time_already_correct_dialog_title">De tijd is al juist</string>
    <string name="timesync_sync_now_time_already_correct_dialog_details">Interne klok Authenticator werd niet aangepast, omdat hij al gesync. is met de servers.</string>
